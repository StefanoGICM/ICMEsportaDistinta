<Window x:Class="ICM.SWPDM.EsportaDistintaAddin.EsportaDistintaForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ICM.SWPDM.EsportaDistintaAddin"
        xmlns:ICM.ConsoleControlWPF="clr-namespace:ICM.ConsoleControlWPF;assembly=ICM.ConsoleControlWPF" 
        mc:Ignorable="d"
        Title="EsportaDistintaForm" Height="768" Width="1024">
    <Grid Margin="5,2,-1,0" HorizontalAlignment="Right">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="157*"/>
            <ColumnDefinition Width="67*"/>
            <ColumnDefinition Width="171*"/>
            <ColumnDefinition Width="50*"/>
            <ColumnDefinition Width="575*"/>
        </Grid.ColumnDefinitions>
        <ProgressBar x:Name="progBarAnalisi" Height="30" Margin="71,204,10,0" VerticalAlignment="Top" ValueChanged="ProgressBar_ValueChanged" Grid.Column="4" Width="494" HorizontalContentAlignment="Stretch"/>
        <ICM.ConsoleControlWPF:ConsoleControl x:Name="consoleControl" Margin="11,239,9,-58" Grid.ColumnSpan="5"/>
        <Button Content="Esporta Distinta" HorizontalAlignment="Right" Margin="0,214,62,0" VerticalAlignment="Top" Width="148" Click="Button_Click" Grid.ColumnSpan="2"/>
        <TextBox HorizontalAlignment="Left" Margin="11,12,0,0" x:Name ="DistintaTextBox" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="994" Height="22" Grid.ColumnSpan="5"/>
        <TextBox x:Name="ConfigurazioniTextBox" HorizontalAlignment="Left" Margin="22,51,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="825" Grid.Column="1" Height="22" Text="Default" Grid.ColumnSpan="4"/>
        <Label Content="Configurazioni da esportare:" HorizontalAlignment="Left" Margin="14,46,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.145,-0.268" Grid.ColumnSpan="2" Width="178"/>
        <Button Content="Aggiorna Distinta Pregressa" HorizontalAlignment="Left" Margin="377,85,0,0" VerticalAlignment="Top" Width="170" Click="Button_Click_1" Grid.Column="4"/>
        <RadioButton x:Name="RB1" Content="Ultima Versione" Margin="28,106,27,0" VerticalAlignment="Top" Checked="RadioButton_Checked" RenderTransformOrigin="1.866,-1.214" GroupName="GRB1" Click="RB1_Click" IsChecked="True" HorizontalAlignment="Left"/>
        <RadioButton x:Name="RB2" Content="Seleziona Versione" HorizontalAlignment="Left" Margin="28,146,0,0" VerticalAlignment="Top" GroupName="GRB1" Click="RB2_Click"/>
        <RadioButton x:Name="RB3" Content="Ultima Revisione" HorizontalAlignment="Left" Margin="28,126,0,0" VerticalAlignment="Top" GroupName="GRB1" IsChecked="True" Click="RB3_Click"/>
        <RadioButton x:Name="RB4" Grid.Column="1" Content="Ultima Versione" HorizontalAlignment="Left" Margin="33,110,0,0" VerticalAlignment="Top" GroupName="GRB2" Grid.ColumnSpan="2" RenderTransformOrigin="0.484,-0.385" IsChecked="True"/>
        <RadioButton x:Name="RB5" Grid.Column="1" Content="Ultima Revisione" HorizontalAlignment="Left" Margin="33,129,0,0" VerticalAlignment="Top" Width="136" IsChecked="False" GroupName="GRB2" Grid.ColumnSpan="2"/>
        <GroupBox HorizontalAlignment="Left" Height="84" Header="Padre/Radice" Margin="13,86,0,0" VerticalAlignment="Top" Width="144"/>
        <RadioButton x:Name="RB6" Grid.Column="1" Content="Come Costruiti" HorizontalAlignment="Left" Margin="33,146,0,0" VerticalAlignment="Top" Width="102" Grid.ColumnSpan="2" GroupName="GRB2"/>
        <GroupBox Grid.Column="1" Grid.ColumnSpan="2" HorizontalAlignment="Left" Height="84" Header="Figli" Margin="19,86,0,0" VerticalAlignment="Top" Width="134"/>
        <Label x:Name="LabelVerPadre" Content="Versione Padre:" Margin="13,176,53,0" VerticalAlignment="Top" Visibility="Hidden"/>
        <TextBox x:Name="VerPadre" Grid.ColumnSpan="3" HorizontalAlignment="Left" Margin="111,180,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="189" Visibility="Hidden"/>
        <!--<WPF:ConsoleControl x:Name="embeddedConsole" HorizontalAlignment="Center" Margin="0,217,0,0" VerticalAlignment="Top" Height="207" Width="780"/>-->
    </Grid>
</Window>
