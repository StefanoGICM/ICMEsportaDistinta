        <Label x:Name="connARCALabel" Content="Connessione SQL ARCA:" HorizontalAlignment="Left" Margin="45,8,0,0" Grid.Row="1" VerticalAlignment="Top" Width="136" Grid.ColumnSpan="2"/>
        <TextBox x:Name="connFrontiera" Grid.Column="1" Grid.ColumnSpan="5" HorizontalAlignment="Left" Height="22" Margin="22,0,0,0" Grid.Row="1" TextWrapping="Wrap" Text="Data Source='WS22\\SQLSRV2022DEV';Initial Catalog = ICMSWData; User ID = sa; Password = 'P@ssw0rd'; MultipleActiveResultSets=True" VerticalAlignment="Center" Width="768"/>
        <Label Content="Connessione tabelle di frontiera:" HorizontalAlignment="Left" Margin="0,32,0,0" Grid.Row="1" VerticalAlignment="Top" Width="181" Grid.ColumnSpan="2"/>
