        <TextBox x:Name="FileName" Grid.Column="5" HorizontalAlignment="Left" Height="24" Margin="260,125,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="274" TextChanged="FileName_TextChanged"/>
        <Label Grid.Column="5" Content="Dir:" HorizontalAlignment="Left" Height="25" Margin="230,124,0,0" VerticalAlignment="Top" Width="30"/>
